      <ImageView fitHeight="397.0" fitWidth="668.0" layoutX="66.0" layoutY="148.0" opacity="0.2" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="66.0" AnchorPane.rightAnchor="66.0" />
